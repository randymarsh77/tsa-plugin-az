{
	"compilerOptions": {
	  "target": "ES2021",
	  "module": "commonjs",
	  "outDir": "./dist",
	  "rootDir": "./src",
	  "strict": true,
	  "moduleResolution": "node",
	  "esModuleInterop": true,
	  "resolveJsonModule": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "sourceMap": true
	}
  }